   "source": [
    "def f1(tp, tn, fp, fn):\n",
    "    p = tp / (tp + fp)\n",
    "    r = tp/ (tp+ fn)\n",
    "    f = (2 * p * r)/(p + r)\n",
    "    return p, r, f"
   ]
